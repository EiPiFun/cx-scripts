#!/usr/bin/sh                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                              
/usr/bin/squeue $@ -o "%.8A %.16R %.8j %.5C %.10P %.10u %.2t %.12M   %j" --sort="N,-p"                                                                                                                                                                        
/usr/bin/sinfo -o "%.25n %.14C" --sort="N"


